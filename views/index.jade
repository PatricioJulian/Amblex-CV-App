              .cierre
                // Nombre
                .form-group
                  label#nombreInicia.wb-inv(data-toggle='tooltip', data-placement='right', title='¡Para poder continuar escribí tu nombre!') Tu nombre:
                  input#espacioNombre.input-sm.form-control(type='text', ng-model='nombre', placeholder='¿Cuál es tu nombre?', required='')
                // Apellido
                .form-group
                  label#apellidoInicia(data-toggle='tooltip', data-placement='right', title='¡Para poder continuar escribí tu apellido!') Apellido:
                  input#espacioApellido.input-sm.form-control(type='text', ng-model='apellido', placeholder='¿Cuál es tu Apellido?', required='')
                // DNI
                .form-group
                  label#dniInicia DNI:
                  input#espacioDNI.input-sm(type='number', ng-model='DNI', placeholder='¿Cuál es tu DNI?', required='')
                // Edad
                .form-group
                  label#edadInicia(data-toggle='tooltip', data-placement='right') Edad:
                  input#espacioEdad.input-sm.form-control(type='number', ng-model='edad', placeholder='¿Cuál es tu edad?', required='')
                // Estado Civil
                .form-group
                  label(for='estadoCivil')  Estado Civil:
                  select.form-control(name='estadoCivil', ng-model='data.estadoCivil', required='')
                    option(value='Soltero/a') Soltero/a
                    option(value='Casado/a') Casado/a
                    option(value='Divorciado/a') Divorciado/a
                    option(value='Viudo/a') Viudo/a
                // Celular
                .form-group
                  label#celularInicia(data-toggle='tooltip', data-placement='right', title='¡Para poder continuar escribí tu nro de Celular!') Celular:
                  input#espacioCelular.input-sm.form-control(type='number', ng-model='celular', placeholder='¿Cuál es tu nro de Celular?', required='')
                // Fijo
                .form-group
                  label#fijoInicia(data-toggle='tooltip', data-placement='right', title='¡Para poder continuar escribí tu nro de telefono fijo!') Telefono:
                  input#espacioFijo.input-sm.form-control(type='number', ng-model='fijo', placeholder='¿Cuál es tu nro de telefono?')
                // Email
                .form-group
                  i.fa.fa-envelope-o.fa-fw
                  label#emailInicia(data-toggle='tooltip', data-placement='right', title='¡Para poder continuar escribí tu email!') Email:
                  input#espacioFijo.input-sm.form-control(type='email', ng-model='email', placeholder='¿Cuál es tu email?', data-error='Porfavor escriba una dirección de email correcta', required='')
